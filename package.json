{
	"name": "react-boilerplate",
	"version": "2.0.0",
	"description": "",
	"type": "module",
	"scripts": {
		"start": "vite",
		"build": "vite build",
		"preview": "vite build && vite preview",
		"postinstall": "husky install",
		"lint": "eslint --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
		"lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
		"validate": "npx lint-staged",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/canderis/react-boilerplate.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/canderis/react-boilerplate/issues"
	},
	"homepage": "https://github.com/canderis/react-boilerplate#readme",
	"lint-staged": {
		"*.{js, jsx,ts,tsx}": [
			"eslint --quiet --fix"
		],
		"*.{json,js,ts,jsx,tsx,html}": [
			"prettier --write --ignore-unknown"
		]
	},
	"devDependencies": {
		"@eslint/js": "9.20.0",
		"@types/lodash": "4.17.15",
		"@types/node": "22.13.4",
		"@types/react": "19.0.9",
		"@types/react-dom": "19.0.3",
		"@types/react-router-config": "5.0.11",
		"@types/react-router-dom": "5.3.3",
		"@vitejs/plugin-react": "4.3.4",
		"eslint": "9.20.1",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-css": "0.11.0",
		"eslint-plugin-prettier": "5.2.3",
		"eslint-plugin-react": "7.37.4",
		"globals": "15.15.0",
		"husky": "9.1.7",
		"lint-staged": "15.4.3",
		"prettier": "3.5.1",
		"tailwindcss": "4.0.6",
		"typescript": "5.7.3",
		"typescript-eslint": "8.24.0",
		"vite": "6.1.0"
	},
	"dependencies": {
		"@tailwindcss/vite": "4.0.6",
		"@tanstack/react-query": "5.66.3",
		"lodash": "4.17.21",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-error-boundary": "5.0.0",
		"react-router": "7.1.5",
		"react-router-dom": "7.1.5"
	}
}
